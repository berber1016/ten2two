{
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "overrides": [{
        "files": ".prettierrc",
        "options": { "parser": "json" }
    }],
    "importOrder": [
        "react",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true
}